{
  "name": "testing-sementic-with-next",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "semantic-release": "semantic-release",
    "cm": "npx git-cz",
    "cm:retry": "npx git-cz --retry",
    "commit": "cz"
  },
  "dependencies": {
    "commitizen": "^4.2.4",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.2",
    "semantic-release": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KhanJr/testing-sementic-release.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "maxHeaderWidth": 100,
      "maxLineWidth": 100,
      "defaultType": "",
      "defaultScope": "",
      "defaultSubject": "",
      "defaultBody": "",
      "defaultIssues": ""
    }
  },
  "release": {
    "branches": [
      "master",
      "main"
    ]
  }
}
